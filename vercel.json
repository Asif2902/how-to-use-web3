{
  "version": 2,
  "builds": [
    {
      "src": "server.js", // Your main server file
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/blog",
      "methods": ["GET"],
      "dest": "/server.js" // Route all blog requests to your Express server
    },
    {
      "src": "/(.*)",
      "dest": "/server.js" // Route all other requests to your Express server
    }
  ]
}
